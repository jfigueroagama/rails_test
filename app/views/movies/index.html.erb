<div id = "content" class = "movies">
<h1>Movie index</h1>

<%= form_for :search, method: :get do |form| %>
  <p>
    <%= form.text_field :query %>
    <%= form.submit "Search", name: nil, class:"btn btn-primary"%>
  </p>
<% end %>

<% @movies.each do |movie| %>
	<%= link_to movie.title %><br />
<% end %>


</div>
